
public class Card extends Deck {
Rank rank;
Suit suit;
private int value;

public Card(Rank rank, Suit suit) {
	this.rank = rank;
	this.suit = suit;
	this.value = value;
}

public Rank getRank() {
	return rank;
}

public Suit getSuit() {
	return suit;
}

@Override
public String toString() {
	return rank + " of " + suit +"'s";
}



}
