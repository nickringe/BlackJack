
public class Dealer {

	private int card1Value;
	private int card2Value;
	
	public void Dealer(int card1Value, int card2Value) {
		this.card1Value = card1Value;
		this.card2Value = card2Value;
	}

	public int getCard1Value() {
		return card1Value;
	}

	public int getCard2Value() {
		return card2Value;
	}
	
	
}
