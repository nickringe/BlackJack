
public class Dealer {

	private Card card1;
	private Card card2;
	
	public Dealer(Card card1, Card card2) {
		this.setCard1(card1);
		this.card2 = card2;
	}

	public int getCard1Value() {
		return this.getCard1Value();
	}

	public int getCard2Value() {
		return this.getCard2Value();
	}

	public Card getCard1() {
		return card1;
	}

	public void setCard1(Card card1) {
		this.card1 = card1;
	}
	
	
}
